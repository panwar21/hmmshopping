              <td> My Account </td>


              <td><%= link_to "Login Page", {controller: "my_accounts", action: "loginpage"}, class: "labels" %></td>
              <td><%= link_to "logout", {controller: "my_accounts", action: "logout"}, class: "labels" %></td>

              <td><%= link_to "View History", {controller: "my_accounts", action: "viewhistory"}, class: "labels" %></td>

