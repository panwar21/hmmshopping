              <td><%= link_to "Tools", {controller: "technicalinfos", action: "tools"}, class: "labels" %></td>

              <td><%= link_to "Downloads", {controller: "technicalinfos", action: "downloads"}, class: "labels" %></td>

            </tr>


            <tr>
                      
              <td> Keep me posted </td>


              <td><%= link_to "Subscribe", {controller: "keepmeposteds", action: "subscribe"}, class: "labels" %></td>
              <td><%= link_to "View Notifications", {controller: "keepmeposteds", action: "viewnotifications"}, class: "labels" %></td>

            </tr>


