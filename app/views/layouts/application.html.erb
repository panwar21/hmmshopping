              <td><%= link_to "Post a Review or Comment", {controller: "forum_posts", action: "postareviewncomment"}, class: "labels" %></td>
            </tr>

            <tr>

              <td> Customer Services </td>


              <td><%= link_to "Contact Us", {controller: "customerservices", action: "contactus"}, class: "labels" %></td>
              <td><%= link_to "FAQs", {controller: "customerservices", action: "faqs"}, class: "labels" %></td>

              <td><%= link_to "Help", {controller: "customerservices", action: "help"}, class: "labels" %></td>
